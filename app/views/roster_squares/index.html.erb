<p id="notice"><%= notice %></p>

<h1>Roster Squares</h1>

<table>
  <thead>
    <tr>
      <th>Square</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roster_squares.each do |roster_square| %>
      <tr>
        <td><%= roster_square.square_id %></td>
        <td><%= roster_square.student_id %></td>
        <td><%= link_to 'Show', roster_square %></td>
        <td><%= link_to 'Edit', edit_roster_square_path(roster_square) %></td>
        <td><%= link_to 'Destroy', roster_square, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roster Square', new_roster_square_path %>
